{
  "name": "party-mode",
  "version": "0.1.0",
  "description": "Party Mode for YouTube",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run install-deps",
    "build": "concurrently \"npm run build:server\" \"npm run build:player\" \"npm run build:client\"",
    "build:dev": "concurrently \"npm run build:server\" \"npm run build:player:dev\" \"npm run build:client:dev\"",
    "postbuild": "node build-tasks/copy-views.js",
    "build:server": "tsc",
    "build:player": "cd dependentProjects/playerclient && npm run build:prod",
    "build:client": "cd dependentProjects/mobileclient && npm run build:prod",
    "build:player:dev": "cd dependentProjects/playerclient && npm run build",
    "build:client:dev": "cd dependentProjects/mobileclient && npm run build",
    "buildstart": "npm run build && npm run start",
    "install-deps": "concurrently \"npm run install-deps:player\" \"npm run install-deps:client\"",
    "install-deps:player": "cd dependentProjects/playerclient && npm install",
    "install-deps:client": "cd dependentProjects/mobileclient && npm install",
    "lint": "tslint --project .",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {},
  "keywords": [
    "PartyMode",
    "YouTube",
    "Music",
    "Video",
    "Party"
  ],
  "author": "Mark Benson",
  "license": "UNLICENSED",
  "dependencies": {
    "ajv": "^6.9.2",
    "atob": "^2.1.2",
    "cookie-parser": "^1.4.3",
    "copyfiles": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "gaxios": "^2.0.1",
    "googleapis": "^43.0.0",
    "html-entities": "^1.2.1",
    "ip": "^1.1.5",
    "jquery": "^3.5.0",
    "moment": "^2.29.2",
    "randomstring": "^1.1.5",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "youtube-player": "^5.5.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/ip": "0.0.31",
    "@types/jquery": "^3.3.29",
    "@types/randomstring": "^1.1.6",
    "@types/request-promise": "^4.1.42",
    "@types/uuid": "^3.4.4",
    "concurrently": "^5.2.0",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.3.12"
  },
  "heroku-run-build-script": true
}
